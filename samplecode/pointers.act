BYTE glob

PROC locPtrInit()
  CARD POINTER ptr = $0004

  DEVPRINT(ptr^)
RETURN

PROC main ()
  BYTE loc
  BYTE POINTER pGlob
  BYTE POINTER pLoc

  locPtrInit()

  pGlob = @glob
  pLoc = @loc

  DEVPRINT(pGlob)
  DEVPRINT(pLoc)

  pGlob^ = 12
  pLoc^ = 24

  DEVPRINT(pGlob^)
  DEVPRINT(pLoc^)
RETURN
